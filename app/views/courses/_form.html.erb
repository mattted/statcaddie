<%= form_with model: course, local: true do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :city %>
    <%= f.text_field :city, class: "form-control"  %>
  </div>

  <div class="form-group">
    <%= f.label :state %>
    <%= f.text_field :state, class: "form-control"  %>
  </div>

  <div class="form-group">
    <%= f.label :style %><br>
    <%= f.collection_select :style, Course.all, :style, :style, { include_blank: true }, { class: "form-control" } %>
  </div>

  <div class="form-group">
    <%= f.label :access %>
    <%= f.collection_select :access, Course.all, :access, :access, { include_blank: true }, { class: "form-control" } %>
  </div>

  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>

