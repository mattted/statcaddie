<%= form_with model: [course, tee], local: true do |f| %>
  <div class="form-group">
    <%# <%= f.label :color, "Tee Color" %1> %>
    <%= f.text_field :color, class: "form-control", placeholder: "Tee color..." %>
  </div>

  <div class="container mb-4">
    <div class="row">
      <%= f.fields_for :holes do |ff| %>
        <%= render 'holes/form_fields', f: ff %>
      <% end %>
    </div>
  </div>

  <%= f.submit "Create Tee", class: "btn btn-primary" %>

<% end %>


